@using MotorRegister.Api
@using MotorRegister.Core.Entities
@using MotorRegister.Web.Components.Cards

<div class="card-wrapper">
    @foreach (VehicleDTO item in Items)
    {
        <VehicleCard Vehicle="item"></VehicleCard>
    }
</div>

@code {
    [Parameter]
    public List<VehicleDTO> Items { get; set; }
}